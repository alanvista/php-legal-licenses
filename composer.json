{
    "name": "alanvista/php-legal-licenses",
    "description": "A utility to generate a Licenses file containing the full license text for every dependency in your project for legal purposes., fork for  https://github.com/Comcast/php-legal-licenses",
    "type": "library",
    "minimum-stability": "stable",
    "version" : "1.00",
    "keywords": [
        "licenses",
        "license",
        "legal",
        "utility",
        "binary"
    ],
    "homepage": "https://github.com/alansslva/php-legal-licenses",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Alan Santos",
        "email": "alan.silva@vistasoft.com.br",
        "role": "lead"
    }],
    "support": {
        "issues": "https://github.com/alanvista/php-legal-licenses/issues"
    },
    "prefer-stable": true,
    "require": {
        "ext-json": "*",
        "symfony/console": "~2.3|~3.0|~4.0|~5.0|~6.0",
        "symfony/process": "~2.3|~3.0|~4.0|~5.0|~6.0"
    },
    "bin": [
        "bin/php-legal-licenses"
    ],
    "autoload": {
        "classmap": [
            "src/"
        ]
    }
}
